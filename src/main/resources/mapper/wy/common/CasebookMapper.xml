<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.wy.common.mapper.CasebookMapper">
    <resultMap id="BaseResultMap" type="com.example.wy.common.entity.Casebook" >
        <result column="nationality" property="nationality" />
        <result column="birthplace" property="birthplace" />
        <result column="admission" property="admission" />
        <result column="writetime" property="writetime" />
        <result column="representor" property="representor" />
        <result column="phone" property="phone" />
        <result column="office" property="office" />
        <result column="mainsuit" property="mainsuit" />
        <result column="PHI" property="PHI" />
        <result column="PH" property="PH" />
        <result column="firsttime" property="firsttime" />
        <result column="hospital" property="hospital" />

    </resultMap>
    <select id="selectAllByPhone" resultType="com.example.wy.common.entity.Casebook">
        select * from casebook where phone=#{phone}
    </select>
</mapper>
